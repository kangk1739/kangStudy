	<script type="text/javascript" src="../assets/js/jquery-3.5.1.min.js"></script>
	<%
		SolarDao sdao = new SolarDao();
		int[] dataArr = new int[17];
		dataArr = sdao.solarMap();
	%>
	<script>
	var dataArray = [];
	<%for(int i=0; i<dataArr.length; i++){%>
	dataArray.push(<%=dataArr[i]%>);
	<%}%>
	
	mapColor(dataArray);
	function mapColor(dataArrayIn){
		var OUTLINE = $('.OUTLINE');
		for(var i=0; i<dataArrayIn.length; i++){
			$($('.dataTable')[i]).text(dataArrayIn[i]); //span 태그에 데이터 넣기
			if(dataArrayIn[i] < 50){
				$(OUTLINE[i]).css({'fill':'#eee'});
			}else if(dataArrayIn[i] < 150){
				$(OUTLINE[i]).css({'fill':'#FFFFC6'});
			}else if(dataArrayIn[i] < 300){
				$(OUTLINE[i]).css({'fill':'#FFEE90'});
			}else if(dataArrayIn[i] < 450){
				$(OUTLINE[i]).css({'fill':'#FFCA6C'});
			}else if(dataArrayIn[i] < 600){
				$(OUTLINE[i]).css({'fill':'#FFA648'});
			}else if(dataArrayIn[i] < 750){
				$(OUTLINE[i]).css({'fill':'#FF8224'});
			}else if(dataArrayIn[i] < 900){
				$(OUTLINE[i]).css({'fill':'#ED4C00'});
			}
		}
	}
	
	$('#solarDataBtn').click(function(){
		var date = $('#solarDataDate');
		var time = $('#solarDataTime');
		if(date.val() == ''){
			alert('날짜를 입력해주세요.');
			return;
		}else if(time.val() == ''){
			alert('시간을 입력해주세요');
			return;
		}
				
		var queryString = date.val()+" "+time.val()+":00:00";
		console.log(queryString);
		
		var request = $.ajax({
		  url: "action/ajaxSolarMap.jsp",
		  method: "POST",
		  data: { queryString : queryString },
		  dataType: "json"
		});
		
		request.done(function( data ) {
		  console.log('json 받기 성공');
		  console.log(data);
		  console.log('json 받기 성공');
		  
		  //json으로 받아온 값으로 지도색깔 메서드 호출
		  mapColor(data);
		});
		 
		request.fail(function( jqXHR, textStatus ) {
		  alert( "Request failed: " + textStatus );
		});
		
		
		//ajax , json 적용이 필요함, script변수로 java 메서드 실행이 필요함
	});	
	
	</script>
	
